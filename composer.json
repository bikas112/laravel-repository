{
    "name": "vxsoft/laravel-repository",
    "description": "A repository package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-repository"
    ],
    "autoload": {
        "psr-4": {
            "Vxsoft\\LaravelRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vxsoft\\LaravelRepository\\Providers\\AppServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    }
}
